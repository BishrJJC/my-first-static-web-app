{
    "navigationFallback": {
      "rewrite": "/index.html"
    },
    "routes": [
      {
        "route": "/*",
        "allowedRoles": [ "authenticated" ]
      },
      {
        "route": "/api/get-search-key",
        "allowedRoles": ["authenticated"],
        "methods": ["GET"]
      }
    ],
    "responseOverrides": {
      "401": {
        "statusCode": 302,
        "redirect": "/.auth/login/aad"
      }
    }
  }